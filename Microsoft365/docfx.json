{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "src": "enterprise",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "src": "security",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "src": "Hub",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg",
          "**/*.pdf"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/troubleshoot/microsoft-365/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "recommendations": true,
      "feedback_system": "Standard",
      "feedback_product_url": "https://admin.microsoft.com/adminportal/home?showfeedback=DocsMacCampaign",
      "hideEdit": true,
      "searchScope": ["Microsoft 365 Support"],
      "uhfHeaderId": "MSDocsHeader-M365-IT",
      "titleSuffix": "Microsoft 365",
      "contributors_to_exclude": [ 
        "beccarobins"
      ]
    },
    "fileMetadata": {
      "ms.service": {
        "admin/**/*.md": "microsoft-365-admin",
        "enterprise/**/*.md": "microsoft-365-enterprise",
        "security/**/*.md": "microsoft-365-security",
        "purview/**/*.md": "purview"      
      },
      "author": {
        "admin/**/*.md": "Cloud-Writer",
        "enterprise/**/*.md": "Cloud-Writer",
        "purview/**/*.md": "cloud-writer",
        "security/**/*.md": "cloud-writer"
      },
      "ms.author": {
        "admin/**/*.md": "meerak",
        "enterprise/**/*.md": "meerak",
        "purview/**/*.md": "meerak",
        "security/**/*.md": "meerak"
      }
    },
    "template": [],
    "dest": "OfficeDocs-Support-Office365",
    "markdownEngineName": "markdig"
  }
}
