{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "src": "Hub",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "src": "Client",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "src": "OfficeExperts",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.pdf",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/office/troubleshoot/breadcrumb/toc.json",
      "recommendations": true,
      "feedback_system": "Standard",
      "feedback_product_url": "https://techcommunity.microsoft.com/",
      "titleSuffix": "Microsoft 365 Apps",
      "uhfHeaderId": "MSDocsHeader-M365-IT",
      "hideEdit": true,
      "searchScope": ["Microsoft 365 Apps Support"],
      "contributors_to_exclude": [ 
        "beccarobins"
      ]
    },
    "fileMetadata": {
      "ms.service": {
        "**/*.md": "office-perpetual-itpro"     
      },
      "author": {
        "**/*.md": "helenclu"
      },
      "ms.author": {
        "**/*.md": "luche"
      }
    },
    "template": [],
    "dest": "OfficeDocs-Support-Office",
    "markdownEngineName": "markdig"
  }
}
