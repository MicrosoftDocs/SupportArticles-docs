{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "src": "Hub",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "src": "SharePointServer",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "src": "SharePointExperts",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "src": "SharePointOnline",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "src": "SharePointHybrid",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "src": "OneDrive",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.pdf",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/sharepoint/support/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "recommendations": true,
      "feedback_system": "none",
      "hideEdit": true,
      "searchScope": "SharePoint Support",
      "titleSuffix": "SharePoint"
    },
    "fileMetadata": {
      "feedback_product_url": {
        "OneDrive/**.md": "https://onedrive.uservoice.com/"
      }
    },
    "template": [],
    "dest": "OfficeDocs-Support-SharePoint",
    "markdownEngineName": "markdig"
  }
}
