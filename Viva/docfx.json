{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md",
          "SECURITY.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.pdf",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/troubleshoot/viva/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "recommendations": true,
      "feedback_system": "Standard",
      "feedback_product_url": "https://techcommunity.microsoft.com/t5/microsoft-viva-blog/bg-p/MicrosoftVivaBlog",
      "titleSuffix": "Viva",
      "ms.service": "viva",
      "hideEdit": true,
      "uhfHeaderId": "MSDocsHeader-M365-IT",
      "searchScope": ["Viva Support"],
      "contributors_to_exclude": [ 
        "beccarobins"
      ]
    },
    "fileMetadata": {
      "ms.service": {
        "Glint/**/*.md": "viva-glint",
        "Learning/**/*.md": "viva-learning",
        "Pulse/**/*.md": "viva-pulse",
        "Connections/**/*.md": "viva-connections"
      },
      "author": {
        "**/*.md": "Cloud-Writer"
      },
      "ms.author": {
        "**/*.md": "meerak"
      }
    },
    "template": [],
    "dest": "OfficeDocs-Support-Viva",
    "markdownEngineName": "markdig"
  }
}
